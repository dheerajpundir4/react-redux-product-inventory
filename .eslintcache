 (!values.email) {\n      errors.email = \"Email Id is required\";\n    }\n    if (!values.password) {\n      errors.password = \"Password is required\";\n    }\n    if (!values.firstName) {\n      errors.fistName = \"fistName Name is required\";\n    }\n    if (!values.lastName) {\n      errors.lastName = \"lastName is required\";\n    }\n    if (!values.mobileNumber) {\n      errors.mobileNumber = \"mobileNumber is required\";\n    }\n    if (!values.location) {\n      errors.location = \"location is required\";\n    }\n\n\n    return errors\n  }\n\n  if(localStorage.getItem(\"isLogin\")==\"true\")\n  return <Redirect to=\"/\" />;\n\n\n  if (isSignUp) {\n    console.log(\"Signup success\")\n    return <Redirect to=\"/success/signUp\" />;\n\n  }\n\n  return (\n    <Container>\n     \n     \n\n     \n      <Card.Body>\n      <h3>SignUp</h3>\n      <Form onSubmit={handleSubmit}>\n\n        <Form.Group as={Row}>\n          <Col sm=\"3\">\n            <Form.Label>Email</Form.Label>\n          </Col>\n          <Col>\n            <Form.Control type=\"email\"\n              name=\"email\" onChange={handleChange}\n              values={values.email} />\n          </Col>\n          <Col>\n            <Form.Text className=\"text-muted\">\n              {errors.email ? errors.email : null}\n            </Form.Text>\n          </Col>\n        </Form.Group>\n\n\n\n\n        <Form.Group as={Row}>\n          <Col sm=\"3\">\n            <Form.Label>Password</Form.Label>\n          </Col>\n          <Col>\n            <Form.Control type=\"password\"\n              name=\"password\" onChange={handleChange}\n              values={values.password} />\n          </Col>\n          <Col>\n            <Form.Text className=\"text-muted\">\n              {errors.password ? errors.password : null}\n            </Form.Text>\n          </Col>\n        </Form.Group>\n\n        <Form.Group as={Row}>\n          <Col sm=\"3\">\n            <Form.Label>First Name</Form.Label>\n          </Col>\n          <Col>\n            <Form.Control type=\"text\"\n              name=\"firstName\" onChange={handleChange}\n              values={values.firstName} />\n\n          </Col>\n          <Col>\n            <Form.Text className=\"text-muted\">\n              {errors.firstName ? errors.firstName : null}\n            </Form.Text>\n          </Col>\n        </Form.Group>\n\n        <Form.Group as={Row}>\n          <Col sm=\"3\">\n            <Form.Label>Last Name</Form.Label>\n          </Col>\n         \n          <Col>\n          <Form.Control type=\"text\" \n            name=\"lastName\" onChange={handleChange}\n            values={values.lastName} />\n\n          </Col>\n          <Col>\n            <Form.Text className=\"text-muted\">\n              {errors.lastName ? errors.lastName : null}\n            </Form.Text>\n          </Col>\n        </Form.Group>\n\n        <Form.Group as={Row}>\n          <Col sm=\"3\">\n            <Form.Label>Location</Form.Label>\n          </Col>        \n          <Col>\n          <Form.Control type=\"text\" \n            name=\"location\" onChange={handleChange}\n            values={values.location} />\n          </Col>\n          <Col>\n            <Form.Text className=\"text-muted\">\n              {errors.location ? errors.location : null}\n            </Form.Text>\n          </Col>\n        </Form.Group>\n\n        <Form.Group as={Row}>\n          <Col sm=\"3\">\n            <Form.Label>Mobile</Form.Label>\n          </Col>       \n\n          <Col>\n          <Form.Control type=\"text\"\n            name=\"mobileNumber\" onChange={handleChange}\n            values={values.mobileNumber} />\n\n          </Col>\n          <Col>\n            <Form.Text className=\"text-muted\">\n              {errors.mobileNumber ? errors.mobileNumber : null}\n            </Form.Text>\n          </Col>\n        </Form.Group>\n\n        <Form.Group as={Row}>\n        <Col sm=\"3\"/>      \n        <Col>\n          <Button variant=\"primary\" type