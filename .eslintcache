st(URL,{\n       id: currentID,\n       productName: product.productName,\n       quantity: product.quantity,\n       price:product.price\n       \n      }).then(response=> {   \n         dispatch(addProductSuccess(product))  \n         window.history.back()    \n     }) \n\n    }   \n} \n\nexport function editProduct(product){  \n  \n  return dispatch=>{    \n   \n    axios.put(URL+\"/\"+product.id,{    \n     \n    productName: product.productName,\n    quantity: product.quantity,\n   price:product.price\n     \n    }).then(response=> {   \n       dispatch(editProductSuccess(product))  \n       window.history.back()    \n   }) \n\n  }   \n} \n\nexport function deleteProduct(id){\n  return dispatch=>{   \n    \n    axios.delete(URL+\"/\"+id).then(response => {   \n      dispatch(deleteProductSuccess(id))  \n      window.history.back()\n    }) \n  \n}\n}","/home/dheeeraj/Desktop/React Assignment/cap/src/actions/actionTypes.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/data/ProductApi.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/index.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/productReducer.js",["102","103"],"import * as types from '../actions/actionTypes';\nimport initialState from './initialState';\n\nexport default function productReducer(state = initialState.products, action) {\n\n  switch (action.type) {\n\n    // via productAction we got all the products\n    case types.LOAD_PRODUCTS_SUCCESS:\n      return action.products;\n\n    case types.ADD_PRODUCT_SUCCESS:     \n      return [\n        ...state,\n        Object.assign({}, action.product)\n      ];\n\n\n    case types.DELETE_PRODUCT_SUCCESS:\n      let newStateDelete = state.filter(product => product.id != action.id); // ES6 arrow funciton     \n      return newStateDelete;\n\n    case types.EDIT_PRODUCT_SUCCESS:\n\n      let newStateEdit = state.map(product => {\n        if (product.id != action.id)\n          return product\n        return action.product\n      });  // ES6 arrow fns     \n      return newStateEdit;\n\n\n    default:\n      return state;\n  }\n}\n","/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/initialState.js",["104","105"],"import ProductApi from '../data/ProductApi';\n\nexport default {\n  products: []\n};","/home/dheeeraj/Desktop/React Assignment/cap/src/components/Login.js",["106"],"import React, { useState } from \"react\";\nimport { Link, Redirect } from 'react-router-dom';\nimport axios from 'axios';\n\n\n// Using bootstrap\nimport Form from 'react-bootstrap/Form';\n\n\nfunction Login() {\n\n  const [isLoggedIn, setLoggedIn] = useState(false);\n  const [isError, setIsError] = useState(false);\n  const [userName, setUserName] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n\n\n \n  function postLogin() {  \n    axios.get('http://localhost:4000/login').then(result => {\n      if (result.status === 200) {\n        // setAuthTokens(result.data);\n        console.log(\"postLogin\")\n        localStorage.setItem('userId','asdf')\n        setLoggedIn(true);\n      } else {\n        console.log(\"elsepostLogin\");\n        localStorage.setItem('userId','asdf')\n        setIsError(true);\n      }\n    }).catch(e => {\n      console.log(\"where is dheeraj\");\n      setIsError(true);\n    });\n\n  }\n\n\n  if (isLoggedIn) {\n    return <Redirect to=\"/products\" />;\n  }\n\n  return (\n    <>\n      { isError && <p>username and passwork is incorrect</p>}\n\n\n      <Form>\n\n        <Form.Group>\n\n          <input\n            type=\"userName\"\n            value={userName}\n            onChange={e => {\n              setUserName(e.target.value);\n            }}\n            placeholder=\"email\" />\n        </Form.Group>\n        <Form.Group>\n          <input\n            type=\"password\"\n            value=\"password\"\n            onChange={e => {\n              setPassword(e.target.value);\n            }}\n            placeholder=\"password\" />\n        </Form