tatus = 200) {\n         this.setUser(res.data)\n            localStorage.setItem(\"isLogin\",\"true\")\n        }\n      },\n      err => {\n       \n        console.log(\"--ERROR--\")\n        localStorage.setItem(\"isLogin\",\"false\")\n        console.log(err)\n      }\n    )\n  }\n\n\n\n  componentDidMount() {\n\n    console.log(\"App componentDidMount\")\n\n    this.checkUserValidation()\n  }\n\n\n\n\n  render() {\n\n    console.log(\"App render\")\n    return (\n      <Container fluid=\"xl\">\n        <Container>\n\n         \n        </Container>\n\n        <Container>\n\n\n          <Router history={history}>\n          <NavComponent user={this.state.user} />\n            <Switch>\n           \n                      \n              <Route path=\"/about\" component={AboutPage} />\n              <Route path=\"/topViewProduct\" component={TopViewProduct} />\n            \n              <Route path=\"/view/:id\" component={ViewProductPage} />\n                 \n              <Route path=\"/delete/:id\" component={DeleteProductPage} />\n              <Route path=\"/edit/:id\" component={EditProductPage} />\n              <Route path=\"/addProduct\" component={AddProductPage} />\n             \n              <Route path=\"/Login\" component={() => <Login setUser={this.setUser} />} />\n              <Route path=\"/Signup\" component={Signup} />              \n              <Route path=\"/Logout\" component={() => <Logout setUser={this.setUser} />} />\n              <Route path=\"/success/:id\" component={SuccessComponent} />\n              <Route exact path=\"/\" >\n                <AllProductPage/>\n              </Route>\n            </Switch>\n          </Router>\n        </Container>\n      </Container>\n    );\n  }\n}\n\nexport default (App);\n\n","/home/dheeeraj/Desktop/React Assignment/cap/src/store/configureStore.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/actions/productActions.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/actions/actionTypes.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/data/ProductApi.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/index.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/productReducer.js",["127","128"],"import * as types from '../actions/actionTypes';\nimport initialState from './initialState';\n\nexport default function productReducer(state = initialState.products, action) {\n\n  switch (action.type) {\n\n    // via productAction we got all the products\n    case types.LOAD_PRODUCTS_SUCCESS:\n      return action.products;\n\n    case types.ADD_PRODUCT_SUCCESS:     \n      return [\n        ...state,\n        Object.assign({}, action.product)\n      ];\n\n\n    case types.DELETE_PRODUCT_SUCCESS:\n      let newStateDelete = state.filter(product => product.id != action.id); // ES6 arrow funciton     \n      return newStateDelete;\n\n    case types.EDIT_PRODUCT_SUCCESS:\n\n    console.log(\"we are in edit reducer funciton\")\n\n      let newStateEdit = state.map(product => {   \n        if (product.id != action.product.id)\n          return product          \n        return action.product\n      });  // ES6 arrow fns   \n      \n      console.log(newStateEdit)\n      console.log(\"we are in edit reducer end funciton\")\n      return newStateEdit;\n\n\n    default:\n      return state;\n  }\n}\n","/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/initialState.js",["129","130"],"import ProductApi from '../data/ProductApi';\n\nexport default {\n  products: []\n};","/home/dheeeraj/Desktop/React Assignment/cap/src/reportWebVitals.js",[],["131","132"],"/home/dheeeraj/Desktop/React Assignment/cap/src/components/NavComponent.js",["133"],"import React from 'react'\n\n// Using bootstrap\nimport Navbar from 'react-bootstrap/Navbar';\nimport Nav from 'react-bootstrap/Nav';\nimport { Container } from 'react-bootstrap';\n\nimport { Link, NavLink } from 'react-router-dom'\n\nexport default class NavComponent extends React.Component {\n    render() {\n\n        console.log(\"Nav Component render\")\n        let buttons\n        if (this.props.user) {\n            buttons = (\n                <>\n\n\n                    <Nav>\n\n                        <Link class=\"nav-link\" to=\"/addProduct\">Add Product</Link>\n\n\n                        <Link class=\"nav-link\" to=\"/Logout\">Logout</Link>\n\n                    </Nav>\n            \n                </>\n            )\n        }\n        else {\n            buttons = (\n                <>\n\n\n                    <Nav>\n\n                        <Link class=\"nav-link\" to=\"/Login\">Login</Link>\n\n                        <Link class=\"nav-link\" to=\"/Signup\">SignUp</Link>\n\n\n\n                    </Nav>\n\n\n                </>\n            )\n        }\n        return (\n            <Container>\n                <Navbar collapseOnSelect expand=\"lg\" bg=\"blue\" variant=\"light\">\n                    <Navbar.Brand href=\"/\">Product-Inventory</Navbar.Brand>\n                    <Navbar.Toggle aria-controls=\"responsive-navbar-nav\" />\n                    <Navbar.Collapse id=\"responsive-navbar-nav\">\n                        <Nav className=\"mr-auto\">\n                            <Link class=\"nav-link\" to=\"/\">Products</Link>\n                            <Link class=\"nav-link\" to=\"/topViewProduct\">Top View Product</Link>\n                            <Link class=\"nav-link\" to=\"/about\">About</Link>\n\n                        </Nav>\n                       {buttons}\n                    </Navbar.Collapse>\n                </Navbar>                \n            </Container>\n        );\n    }\n}","/home/dheeeraj/Desktop/React Assignment/cap/src/components/SuccessComponent.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/components/AddProduct/AddProductPage.js",["134","135"],"import React from 'react';\nimport { connect } from 'react-redux';\nimport { Redirect } from 'react-router-dom';\nimport { withRouter } fro