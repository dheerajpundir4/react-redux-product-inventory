rs.password = \"Password is required\";\n    }\n\n\n    return errors\n  }\n\n\n  if (isLoggedIn) {\n    return <Redirect to=\"/\" />;\n  }\n\n  return (\n    <Container>\n\n     \n      <Card style={{ width: '50rem' }}>\n    \n     \n<Card.Body>\n<h3> Login </h3>\n      <Form onSubmit={handleSubmit}>\n\n       \n        <Form.Group>\n       \n          <Form.Label>Email</Form.Label>\n      \n         \n          <Form.Control \n          as=\"input\"\n           type=\"email\"\n            name=\"email\" onChange={handleChange}\n            values={values.email} />\n           \n        \n          <Form.Text className=\"text-muted\">\n            {errors.email ? errors.email : null}\n          </Form.Text>\n         \n        </Form.Group>\n\n        <Form.Group>\n          <Form.Label>Password</Form.Label>\n\n          <Form.Control \n          type=\"password\"\n            name=\"password\" onChange={handleChange}\n            values={values.password} />\n          <Form.Text className=\"text-muted\">\n            {errors.password ? errors.password : null}\n          </Form.Text>\n        </Form.Group>\n\n        <Form.Group>\n          <Button variant=\"primary\" type=\"submit\">Sign In</Button>\n        </Form.Group>\n      </Form>\n      </Card.Body>\n      </Card>\n    </Container>\n  )\n}\n\nexport default Login;","/home/dheeeraj/Desktop/React Assignment/cap/src/user/Signup.js",["139","140","141","142","143","144"],"import React, { useState } from \"react\";\nimport { Redirect } from 'react-router-dom';\nimport { Formik, useFormik } from 'formik';\n\n//const URL =\"http://localhost:4000/products\";\n\n\n//It is used to call server directly\nimport axios from 'axios';\n\n\n// Using bootstrap\nimport { Form, Button, Col, Row, Container, Card } from 'react-bootstrap';\n\n\n\nfunction Signup() {\n\n  const [isSignUp, setSignUp] = useState(false);\n  const [isError, setIsError] = useState(false);\n  const [isUserExist, setIsUserExist] = useState(false);\n\n  co