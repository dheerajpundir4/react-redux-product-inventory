\n      err => {\n        console.log(\"=======error===========\")\n        console.log(err)\n      }\n    )\n    console.log(\"++++++++++++++++++++++++\")\n  }\n\n\n  setUser = (us) => {\n    this.setState({\n      user: us\n    })\n  }\n\n  render() {\n    return (\n      <Container fluid=\"xl\">\n        <Container>\n\n          <NavComponent user={this.state.user} />\n        </Container>\n\n        <Container>\n\n\n          <Router>\n            <Switch>\n              <Route path=\"/products\" component={AllProductPage} />\n              <Route path=\"/about\" component={AboutPage} />\n              <Route path=\"/topViewProduct\" component={TopViewProduct} />\n              <Route path=\"/addProduct\" component={()=><AddProductPage user={this.state.user}/>} />\n              <Route path=\"/view/:id\" component={ViewProductPage} />\n              <Route path=\"/delete/:id\" component={DeleteProductPage} />\n              <Route path=\"/edit/:id\" component={EditProductPage} />\n              <Route exact path=\"/\">\n                <AllProductPage />\n              </Route>\n              <Route path=\"/Login\" component={() => <Login setUser={this.setUser} />} />\n              <Route path=\"/Signup\" component={Signup} />\n              <Route path=\"/Logout\" component={Logout} />\n              <Route path=\"/success/:id\" component={SuccessComponent} />\n            </Switch>\n          </Router>\n        </Container>\n      </Container>\n    );\n  }\n}\n","/home/dheeeraj/Desktop/React Assignment/cap/src/store/configureStore.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/actions/productActions.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/actions/actionTypes.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/data/ProductApi.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/index.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/productReducer.js",["123","124"],"/home/dheeeraj/Desktop/React Assignment