0"],"/home/dheeeraj/Desktop/React Assignment/cap/src/store/configureStore.js",[],["121","122"],"/home/dheeeraj/Desktop/React Assignment/cap/src/actions/productActions.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/actions/actionTypes.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/data/ProductApi.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/index.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/productReducer.js",["123","124"],"import * as types from '../actions/actionTypes';\nimport initialState from './initialState';\n\nexport default function productReducer(state = initialState.products, action) {\n\n  switch (action.type) {\n\n    // via productAction we got all the products\n    case types.LOAD_PRODUCTS_SUCCESS:\n      return action.products;\n\n    case types.ADD_PRODUCT_SUCCESS:     \n      return [\n        ...state,\n        Object.assign({}, action.product)\n      ];\n\n\n    case types.DELETE_PRODUCT_SUCCESS:\n      let newStateDelete = state.filter(product => product.id != action.id); // ES6 arrow funciton     \n      return newStateDelete;\n\n    case types.EDIT_PRODUCT_SUCCESS:\n\n    console.log(\"we are in edit reducer funciton\")\n\n      let newStateEdit = state.map(product => {   \n        if (product.id != action.product.id)\n          return product          \n        return action.product\n      });  // ES6 arrow fns   \n      \n      console.log(newStateEdit)\n      console.log(\"we are in edit reducer end funciton\")\n      return newStateEdit;\n\n\n    default:\n      return state;\n  }\n}\n","/home/dheeeraj/Desktop/React Assignment/cap/src/reducers/initialState.js",["125","126"],"import ProductApi from '../data/ProductApi';\n\nexport default {\n  products: []\n};","/home/dheeeraj/Desktop/React Assignment/cap/src/reportWebVitals.js",[],["127","128"],"/home/dheeeraj/Desktop/React Assignment/cap/src/components/NavComponent.js",["129"],"/home/dheeeraj/Desktop/React Assignment/cap/src/componen