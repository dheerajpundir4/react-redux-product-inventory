       \n        values.price=\"Rs. \"+values.price\n    }\n    return errors\n}\n\n\n\n\n    return (\n        <form onSubmit={handleSubmit}>\n            <h1>Add SingleProduct</h1>\n            <div>\n                <input type=\"text\" placeholder=\"Enter SingleProduct Name\" name=\"productName\" onChange={handleChange} values={values.productName} />\n                {errors.productName?errors.productName:null}               \n            </div>\n            <br/>\n            <div>\n                <input type=\"number\" placeholder=\"Enter Quantity\" name=\"quantity\" onChange={handleChange} values={values.quantity}/>\n                {errors.quantity?errors.quantity:null}\n            </div>\n            <br/>\n            <div>\n                <input type=\"number\" placeholder=\"Enter Price\" name=\"price\" onChange={handleChange} values={values.price} />\n                {errors.price?errors.price:null}\n            </div>\n            <br/>\n            <div>\n                <button type=\"submit\">Submit</button>\n            </div>\n        </form>\n    )\n\n}\n","/home/dheeeraj/Desktop/React Assignment/cap/src/components/displayProduct/AllProductPage.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/components/displayProduct/ProductList.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/components/displayProduct/SingleProduct.js",["104"],{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","severity":1,"message":"110","line":2,"column":42,"nodeType":"111","messageId":"112","endLine":2,"endColumn":50},{"ruleId":"109","severity":1,"message":"113","line":2,"column":8,"nodeType":"111","messageId":"112","endLine":2,"endColumn":18},{"ruleId":"109","severity":1,"message":"114","line":11,"column":7,"nodeType":"111","messageId":"112","endLine":11,"endColumn":13},{"ruleId":"109","severity":1,"message":"115","line":2,"column":10,"nodeType":"111","messageId":"112","endLine":2,"endColumn":14},{"ruleId":"116","severity