ableBody customizationCol={this.state.ARRAY_CUSTOM_COLUMN} product={product} />\n        ));\n\n    }\n\n\n\n\n    return (\n\n      <Container>\n\n\n        <Row>\n\n          <Col sm=\"3\">\n            <Form>\n              <Form.Control type=\"text\"\n                onChange={this.handleFilter}\n                placeholder=\"Filter\"\n                value={this.state.VALUE_OF_FILTER_FORM}\n                className=\"mr-sm-2\" />\n\n            </Form>\n          </Col>\n\n          <Col  >\n\n            {<CustomizationField handleCustomizationField={this.handleCustom} />}\n\n          </Col>\n\n\n        </Row>\n\n\n\n        <Table striped bordered hover>\n          <thead>\n            {tableHead}\n          </thead>\n          <tbody>\n            {tableBody}\n          </tbody>\n        </Table>\n\n        <br />\n        <Link to=\"/addProduct\">Add Product</Link>\n        <br />\n\n        <Link to=\"/topViewProduct\">Top View Product</Link>\n      </Container>\n\n\n    );\n  }\n}\n\n\nconst mapStatetoProps = (state) => {\n  return {\n    products: state.products\n  }\n}\n\nexport default connect(mapStatetoProps, { loadProduct })(AllProductListPage);\n","/home/dheeeraj/Desktop/React Assignment/cap/src/pages/AllProduct/CustomizationField.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/components/NavComponent.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/components/SuccessComponent.js",[],"/home/dheeeraj/Desktop/React Assignment/cap/src/pages/AllProduct/TableData.js",[],{"ruleId":"151","severity":1,"message":"152","line":7,"column":9,"nodeType":"153","messageId":"154","endLine":7,"endColumn":20},{"ruleId":"155","replacedBy":"156"},{"ruleId":"157","replacedBy":"158"},{"ruleId":"159","severity":1,"message":"160","line":65,"column":13,"nodeType":"161","messageId":"162","endLine":65,"endColumn":29},{"ruleId":"163","severity":1,"message":"164","line":20,"column":63,"nodeType":"165","messageId":"166","endLine":20,"endColumn":65},{"ruleId":"16