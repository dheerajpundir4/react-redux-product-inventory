           {errors.productName ? errors.productName : null}\n                    </Form.Text>\n                </Col>\n            </Form.Group>\n\n\n           \n\n            <Form.Group as={Row}>\n                <Col sm=\"3\">\n                    <Form.Label>Product Description</Form.Label>\n                </Col>\n                <Col>\n                <input type=\"text\"\n                 placeholder={props.product.productDescription} \n                 name=\"productDescription\" \n                 onChange={handleChange}\n                  values={values.productDescription} />\n              \n                </Col>\n                <Col>\n                    <Form.Text className=\"text-muted\">\n                    {errors.productDescription ? errors.productDescription : null}\n                    </Form.Text>\n                </Col>\n            </Form.Group>\n\n          \n\n            <Form.Group as={Row}>\n                <Col sm=\"3\">\n                    <Form.Label>Manufacturer</Form.Label>\n                </Col>\n                <Col>\n                <input type=\"text\" \n                placeholder={props.product.manufacturer} \n                name=\"manufacturer\" \n                onChange={handleChange}\n                 values={values.manufacturer} />\n               \n                </Col>\n                <Col>\n                    <Form.Text className=\"text-muted\">\n                    {errors.productName ? errors.productName : null}\n                    </Form.Text>\n                </Col>\n            </Form.Group>\n\n          \n\n            <Form.Group as={Row}>\n                <Col sm=\"3\">\n                    <Form.Label>Quantity</Form.Label>\n                </Col>\n                <Col>\n                <input type=\"number\" placeholder={props.product.quantity} name=\"quantity\" onChange={handleChange} values={values.quantity} />\n              \n                </Col>\n                <Col>\n                    <Form.Text className=\"text-muted\">\n                    {errors.quantity ? errors.quantity : null}\n                    </Form.Text>\n                </Col>\n            </Form.Group>\n\n         \n\n            <Form.Group as={Row}>\n                <Col sm=\"3\">\n                    <Form.Label>Price</Form.Label>\n                </Col>\n                <Col>\n                <input type=\"number\" placeholder={props.product.price} name=\"price\" onChange={handleChange} values={values.price} />\n              \n                </Col>\n                <Col>\n                    <Form.Text className=\"text-muted\">\n                    {errors.price ? errors.price : null}\n                    </Form.Text>\n                </Col>\n            </Form.Group>\n\n\n          \n\n            <Form.Group as={Row}>\n                <Col sm=\"3\">\n                 \n                </Col>\n                <Col>\n                <Button variant=\"primary\" type=\"submit\">Submit</Button>\n                </Col>\n                <Col>                   \n                </Col>\n            </Form.Group>\n\n           \n        </Form>\n    )\n\n}\n","/home/dheeeraj/Desktop/React Assignment/cap/src/history.js",[],{"ruleId":"163","severity":1,"message":"164","line":65,"column":13,"nodeType":"165","messageId":"166","endLine":65,"endColumn":29},{"ruleId":"167","severity":1,"message":"168","line":5,"column":5,"nodeType":"169","messageId":"170","endLine":5,"endColumn":14},{"ruleId":"171","replacedBy":"172"},{"ruleId":"173","replacedBy":"174"},{"ruleId":"175","severity":1,"message":"176","line":20,"column":63,"nodeType":"177","messageId":"178","endLine":20,"endColumn":65},{"ruleId":"175","severity":1,"message":"176","line":28,"column":24,"nodeType":"177","messageId":"178","endLine":28,"endColumn":26},{"ruleId":"167","severity":1,"message":"179","line":1,"column":8,"nodeType":"169","messageId":"170","endLine":1,"endColumn":18},{"ruleId":"180","severity":1,"message":"181","line":3,"colu